<!doctype html>
<html class="no-js" lang="en">

<head>

    <!-- Meta -->
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0, minimal-ui">
    <meta name="msapplication-tap-highlight" content="no">
    
    <!-- Site info -->
    <title>{{ $thisPageTitle }}</title>
    <meta name="description" content="{{ $thisPageDescription }}">
    <!--<meta name="keywords" content="">-->
    <link rel="publisher" href="https://plus.google.com/108074626968863874298">
    <link rel="author" href="humans.txt">
    <!--<link rel="canonical" href="http://www.alion.nl">-->
    
    <!-- Schema.org -->
        
    <!-- Twitter -->
    
    <!-- Og -->
    <!--<meta property="og:url" content="http://www.alion.nl">-->
    <meta property="og:type" content="website">     
    <meta property="og:title" content="{{ $thisPageTitle }}">
    <meta property="og:description" name="description" content="{{ $thisPageDescription }}">
    <meta property="og:image" content="img/common/app-common.jpg">
    
    <!-- Mobile -->
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="apple-mobile-web-app-title" content="{{ $thisPageTitle }}">
    
    <!-- Icons -->
    <link rel="apple-touch-icon-precomposed" sizes="180x180" href="img/common/apple-touch-icon-180x180-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="152x152" href="img/common/apple-touch-icon-152x152-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="120x120" href="img/common/apple-touch-icon-120x120-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="76x76" href="img/common/apple-touch-icon-76x76-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="img/common/apple-touch-icon-precomposed.png"> <!-- 57x57 -->
    <link rel="icon" sizes="192x192" href="img/common/android-touch-icon-192x192.png">
    <link rel="shortcut icon" href="favicon.ico">
    
    <!-- Fonts -->

    <!--Load Modernizr-->

    <!-- CSS -->
    <link href="assets/css/main.css" rel="stylesheet">
    
</head>
    
<body>
    
    {# Header #}
    <header class="mainHeader">
        <h1 class="h1">Flat Starter Template</h1>
    </header>
    
    {# Navigation #}
    {% include "partials/_navigation.nunjucks" %}
    
    {# Content #}
    {% block content %}{% endblock %}
    
    {# Footer #}
    {% include "partials/_footer.nunjucks" %}
    
    {# Scripts #}
    {% include "partials/_scripts.nunjucks" %}
    
</body>
    
</html>